{
  "port": {{PORT}},
  "http_auth": {
    "enabled": {{true|false}},
    "user": "{{HTTP_AUTH_USER}}",
    "password": "{{HTTP_AUTH_PASSWORD}}"
  },
  "www": "{{PATH_TO_CLIENT}}",
  "sites": [{
    "id": "{{SITE_ID}}",
  	"weatherKey": "{{OPEN_WEATHER_API_KEY}}",
    "key": "{{SITE_API_KEY}}",
    "language": "{{LOCATION_FOR_UI_TEXT}}",
    "location": {
    	"city": "{{INITAL_SITE_LOCATION}}"
    }
  }]
}
